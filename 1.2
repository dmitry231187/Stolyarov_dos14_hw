#!/bin/bash

arr1=("foo" "bar" "blah" "test" "123" "smad" "dasd" "foo")
arr2=("bar" "blah" "test" "1223" "ss" "foo" "dfd")
k=0
m=0
for i in "${arr1[@]}"
do
 for j in "${arr2[@]}"
 do
  if [ $i = $j ]; then
   arr3[$k]=$i
   flag=0
   for l in "${arr3[@]}"
   do
    if [ $l = $i ] ; then
     flag=$(($flag+1))
    fi
   done
   if [ $flag -eq 1 ] ; then
    returns[$m]=$i
    m=$(($m+1))
   fi
   k=$(($k+1))
  fi
 done
done

echo -ne 'arr1: ' ${arr1[@]} '\n'
echo -ne 'arr2: ' ${arr2[@]} '\n'
echo -ne 'returns: ' ${returns[@]} '\n'
